{
  "name": "bookmarker-ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "A comprehensive bookmarking platform with AI-powered features for organizing, discovering, and sharing web content.",
  "workspaces": [
    "apps/*",
    "packages/node/*",
    "packages/browser/*",
    "packages/universal/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build": "npm run build --workspaces --if-present",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "npm run typecheck --workspaces --if-present"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matttelliott/bookmarker-ai.git"
  },
  "keywords": [
    "bookmarks",
    "ai",
    "typescript",
    "monorepo"
  ],
  "author": "Matt Elliott",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/matttelliott/bookmarker-ai/issues"
  },
  "homepage": "https://github.com/matttelliott/bookmarker-ai#readme",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "devDependencies": {
    "@jest/types": "^30.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2"
  }
}
